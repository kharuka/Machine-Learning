    "import matplotlib.pyplot as plt\n",
    "%matplotlib inline\n",
    "\n",
    "from scipy.stats import t\n",
    "\n",
    "import numpy as np\n",
    "\n",
    "x = np.linspace(-5,5,100)\n",
    "\n",
    "# 自由度3のt分布\n",
    "rv = t(3)\n",
    "\n",
    "# 確率密度関数（PDF）を書きます。\n",
    "plt.plot(x, rv.pdf(x))"
   ]
  }
